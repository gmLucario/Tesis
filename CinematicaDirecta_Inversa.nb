(*Vectores posición*)
p2 = {l1 Cos[\[Theta]1] + l5/2, l1 Sin[\[Theta]1]};
p4 = {l4 Cos[\[Theta]4] - l5/2, l4 Sin[\[Theta]4]};
P1 = {15/2, 0};
P3 = {X3, Y3};
P5 = {-(l5/2), 0};

(*Vectores de los puntos P2P3 y P4P3*)
VP2P3 = P3 - p2 ;
  VP2P3 // MatrixForm
 
VP4P3 = P3 - p4;
  VP4P3 // MatrixForm
 
(*Ecuaciones restricción*)
EcL2 = VP2P3[[1]]^2 + VP2P3[[2]]^2 - l2^2 == 0;
EcL3 = VP4P3[[1]]^2 + VP4P3[[2]]^2 - l3^2 == 0;
  {EcL2, EcL3} // MatrixForm

(*Igualando las ecuaciones anteriores y despejando a X3*)
P3X = X3 /. Solve[EcL2[[1]] == EcL3[[1]], X3][[1]]
  Collect[P3X, Y3]
  P3Xred = (g) Y3 + f
  
(*Para conocer a Y3*)
EcL2 /. X3 -> P3Xred
SolP3Y = Solve[EcL2 /. X3 -> P3Xred, Y3] // Simplify;
  SolP3Y // MatrixForm
  
  
  
  
 f= (l1^2-l2^2+l3^2-l4^2+l1 l5 Cos[\[Theta]1]+l4 l5 Cos[\[Theta]4])/(2 (l5+l1 Cos[\[Theta]1]-l4 Cos[\[Theta]4]))
